{
	"coffeescript_error": {
		"level": "error"
	},
	"arrow_spacing": {
		"name": "arrow_spacing",
		"level": "warn"
	},
	"braces_spacing": {
		"name": "braces_spacing",
		"level": "warn",
		"spaces": 0,
		"empty_object_spaces": 0
	},
	"camel_case_classes": {
		"name": "camel_case_classes",
		"level": "error"
	},
	"colon_assignment_spacing": {
		"name": "colon_assignment_spacing",
		"level": "warn",
		"spacing": {
			"left": 0,
			"right": 1
		}
	},
	"cyclomatic_complexity": {
		"name": "cyclomatic_complexity",
		"level": "ignore",
		"value": 10
	},
	"duplicate_key": {
		"name": "duplicate_key",
		"level": "error"
	},
	"empty_constructor_needs_parens": {
		"name": "empty_constructor_needs_parens",
		"level": "ignore"
	},
	"non_empty_constructor_needs_parens": {
		"name": "non_empty_constructor_needs_parens",
		"level": "ignore"
	},
	"ensure_comprehensions": {
		"name": "ensure_comprehensions",
		"level": "error"
	},
	"indentation": {
		"name": "indentation",
		"value": 1,
		"level": "error"
	},
	"line_endings": {
		"name": "line_endings",
		"level": "ignore",
		"value": "unix"
	},
	"max_line_length": {
		"name": "max_line_length",
		"value": 80,
		"level": "ignore",
		"limitComments": true
	},
	"missing_fat_arrows": {
		"name": "missing_fat_arrows",
		"level": "ignore"
	},
	"newlines_after_classes": {
		"name": "newlines_after_classes",
		"value": 3,
		"level": "ignore"
	},
	"no_backticks": {
		"name": "no_backticks",
		"level": "error"
	},
	"no_debugger": {
		"name": "no_debugger",
		"level": "warn"
	},
	"no_empty_functions": {
		"name": "no_empty_functions",
		"level": "error"
	},
	"no_empty_param_list": {
		"name": "no_empty_param_list",
		"level": "error"
	},
	"no_implicit_braces": {
		"name": "no_implicit_braces",
		"level": "ignore",
		"strict": true
	},
	"no_implicit_parens": {
		"name": "no_implicit_parens",
		"level": "ignore"
	},
	"no_interpolation_in_single_quotes": {
		"name": "no_interpolation_in_single_quotes",
		"level": "error"
	},
	"no_plusplus": {
		"name": "no_plusplus",
		"level": "ignore"
	},
	"no_stand_alone_at": {
		"name": "no_stand_alone_at",
		"level": "ignore"
	},
	"no_tabs": {
		"name": "no_tabs",
		"level": "ignore"
	},
	"no_throwing_strings": {
		"name": "no_throwing_strings",
		"level": "error"
	},
	"no_trailing_semicolons": {
		"name": "no_trailing_semicolons",
		"level": "error"
	},
	"no_trailing_whitespace": {
		"name": "no_trailing_whitespace",
		"level": "error",
		"allowed_in_comments": false,
		"allowed_in_empty_lines": false
	},
	"no_unnecessary_double_quotes": {
		"name": "no_unnecessary_double_quotes",
		"level": "ignore"
	},
	"no_unnecessary_fat_arrows": {
		"name": "no_unnecessary_fat_arrows",
		"level": "warn"
	},
	"non_empty_constructor_needs_parens": {
		"name": "non_empty_constructor_needs_parens",
		"level": "error"
	},
	"prefer_english_operator": {
		"name": "prefer_english_operator",
		"level": "ignore"
	},
	"space_operators": {
		"name": "space_operators",
		"level": "warn"
	},
	"spacing_after_comma": {
		"name": "spacing_after_comma",
		"level": "warn"
	},

	"forbidden_keywords": {
		"module": "coffeelint-forbidden-keywords",
		"level": "error",
		"forbidden": {
			"yes": "true",
			"no": "false",
			"on": "true",
			"off": "false",
			"is": "==",
			"isnt": "!=",
			"&&": "and",
			"||": "or",
			"!": "not"
		}
	},
	"internal_variable_names_usage": {
		"module": "coffeelint-internal-variable-names-usage",
		"level": "warn"
	},
	"variable_scope": {
		"module": "coffeelint-variable-scope",
		"scopeDiff": 1,
		"level": "ignore"
	},
	"newline_after_function": {
		"module": "coffeelint-newline-after-function",
		"value": 0,
		"level": "error"
	},
	"undefined_variables": {
		"module": "coffeelint-undefined-variables",
		"level": "warn",
		"environments": {
			"browser": true,
			"devel": true,
			"typed": true,
			"jquery": true
		},
		"globals": [
			"_",
			"define",
			"require",
			"postal",
			"Intercom",
			"angular",
			"module",
			"inject",
			"moment",
			"CryptoJS",
			"Modernizr",
			"Promise",
			"File",
			"FileList",
			"Blob",
			"beforeEach",
			"describe",
			"it",
			"expect",
			"spyOn"
		]
	}
}
